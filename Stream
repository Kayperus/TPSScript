 local k = { "\75\97\121\84\80\83",  "\97\99\99\102\102\111\115\108", } local function r() if not m() then g:Kick("\89\111\117\32\97\114\101\32\110\111\116\32\119\104\105\116\101\108\105\115\116\101\100\32\116\111\32\117\115\101\32\116\104\105\115\32\102\101\97\116\117\114\101\44\32\98\117\121\32\116\104\105\115\32\118\105\97\32\95\107\97\121\103\111\97\116\101\100\32\111\110\32\100\105\115\99\111\114\100\46") end end r() local c = game:GetService("\82\117\110\83\101\114\118\105\99\101") local i = game:GetService("\80\108\97\121\101\114\115") local s = game:GetService("\84\119\101\101\110\83\101\114\118\105\99\101") local q = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local g = i.LocalPlayer local _ = workspace.CurrentCamera local r_ = workspace:WaitForChild("\84\80\83\83\121\115\116\101\109"):WaitForChild("\84\80\83") local mr = game.Workspace.RedScore local am = game.Workspace.BlueScore local ea = Instance.new("\80\97\114\116") ea.Name = "\76\111\111\107\66\97\108\108" ea.Anchored = true ea.Transparency = 1 ea.CanCollide = false ea.Position = Vector3.new(0.032, 121.991, 1.308) ea.Parent = workspace local oe = Instance.new("\80\97\114\116") oe.Name = "\66\114\111\97\100\99\97\115\116" oe.Transparency = 1 oe.CanCollide = false oe.Anchored = true oe.Position = Vector3.new(0, 50, 0) oe.Parent = workspace local n = Vector3.new(-25, 35, 0) local ln = 50 local tl = 30 local d = 100 local fd = false local jf = nil local bj = 0 local pb = false local function m() for _, si in ipairs(k) do if g.Name == si then return true end end return false end local function a(e) local kp = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut) local ck = { CFrame = e } local ic = s:Create(_, kp, ck) ic:Play() end local function o(t) for _, g in ipairs(i:GetPlayers()) do if g.DisplayName == t then return g.Name end end return nil end local function l(t) local si = o(t) local qs = g:FindFirstChild("\80\108\97\121\101\114\71\117\105") and g.PlayerGui:FindFirstChild("\83\116\97\114\116") and g.PlayerGui.Start:FindFirstChild("\83\99\111\114\101\115") and g.PlayerGui.Start.Scores:FindFirstChild("\84\105\109\101\114") local gq = qs and qs.Text or "\63\63\58\63\63" local _g = g:FindFirstChild("\80\108\97\121\101\114\71\117\105") and g.PlayerGui:FindFirstChild("\83\116\97\114\116") and g.PlayerGui.Start:FindFirstChild("\73\109\97\103\101\83") and g.PlayerGui.Start.ImageS:FindFirstChild("\65\115\115\105\115\116") and g.PlayerGui.Start.ImageS.Assist:FindFirstChild("\65\115\115\105\115\116") local r_g = _g and _g.Text or nil local mr_ = r_g and o(r_g) or "\78\111\110\101" local amr = g:FindFirstChild("\80\108\97\121\101\114\71\117\105") and g.PlayerGui:FindFirstChild("\83\116\97\114\116") and g.PlayerGui.Start:FindFirstChild("\73\109\97\103\101\83") and g.PlayerGui.Start.ImageS:FindFirstChild("\83\99\111\114\101\100\68\105\115\116\97\110\99\101") and g.PlayerGui.Start.ImageS.ScoredDistance:FindFirstChild("\68\105\115\116\97\110\99\101") local eam = amr and amr.Text or "\63\63\109" if si then print("\91\71\111\97\108\82\101\112\108\97\121\93\32\70\111\117\110\100\32\117\115\101\114\110\97\109\101\58", si, "\97\116\32\116\105\109\101\58", gq) print("\91\71\111\97\108\82\101\112\108\97\121\93\32\65\115\115\105\115\116\32\98\121\58", mr_) print("\91\71\111\97\108\82\101\112\108\97\121\93\32\68\105\115\116\97\110\99\101\58", eam) jf = si bj = tick() + 5 fd = true else warn("\91\71\111\97\108\82\101\112\108\97\121\93\32\79\119\110\32\103\111\97\108\32\111\114\32\110\111\98\111\100\121") end end local oea = game.Workspace.TPSSystem.Part1 local no = game.Workspace.TPSSystem.Part2 oea.Touched:Connect(function(f) if f.Name == "\84\80\83" then local tln = game.Workspace:FindFirstChild("\83\99\111\114\101\114\66") if tln then l(tln.Value) end end end) no.Touched:Connect(function(f) if f.Name == "\84\80\83" then local tln = game.Workspace:FindFirstChild("\83\99\111\114\101\114\82") if tln then l(tln.Value) end end end) warn("\84\111\111\108\32\105\115\32\115\116\105\108\108\32\105\110\32\100\101\118\101\108\111\112\109\101\110\116\44\32\114\101\112\111\114\116\32\98\117\103\115\32\118\105\97\32\95\107\97\121\103\111\97\116\101\100\32\111\110\32\100\105\115\99\111\114\100\46") print("\75\101\121\98\105\110\100\115\58") print("\65\108\116\32\43\32\80\32\58\32\69\110\97\98\108\101\47\68\105\115\97\98\108\101\32\98\114\111\97\100\99\97\115\116") q.InputBegan:Connect(function(j, b) if b then return end if j.KeyCode == Enum.KeyCode.P and q:IsKeyDown(Enum.KeyCode.LeftAlt) then pb = not pb if pb then _.CameraType = Enum.CameraType.Scriptable print("\91\66\114\111\97\100\99\97\115\116\93\32\69\110\97\98\108\101\100") else local qsi = i.LocalPlayer if qsi and qsi.Character and qsi.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local gqs = qsi.Character.HumanoidRootPart.Position local r_gq = gqs + n  oe.CFrame = oe.CFrame:Lerp(CFrame.new(r_gq), ln * 0.1)  oe.CFrame = CFrame.lookAt(oe.Position, gqs + Vector3.new(0, 5, 0))  a(oe.CFrame)  end wait(2) _.CameraType = Enum.CameraType.Custom print("\91\66\114\111\97\100\99\97\115\116\93\32\68\105\115\97\98\108\101\100") end end end) c.RenderStepped:Connect(function(p) if not pb then return end if not r_ or not r_:IsDescendantOf(workspace) then return end local bjf = r_.Position local pbj = bjf.Y local kpb = r_.Velocity.Magnitude local ckp = math.clamp(kpb / d, 0, 1) local ick = tl * ckp local sic = n - Vector3.new(ick, ick * 0.4, 0) if fd and tick() <= bj then local qsi = i:FindFirstChild(jf) if qsi and qsi.Character and qsi.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local gqs = qsi.Character.HumanoidRootPart.Position local r_gq = gqs + sic oe.CFrame = oe.CFrame:Lerp(CFrame.new(r_gq), p * ln)  oe.CFrame = CFrame.lookAt(oe.Position, gqs + Vector3.new(0, 5, 0))  a(oe.CFrame) return else print("\91\71\111\97\108\82\101\112\108\97\121\93\32\85\110\97\98\108\101\32\116\111\32\102\111\108\108\111\119\32\112\108\97\121\101\114\46\32\84\114\121\105\110\103\32\97\103\97\105\110\46") end else fd = false end if pbj >= 11 then local r_gq = bjf + sic oe.CFrame = oe.CFrame:Lerp(CFrame.new(r_gq), p * ln)  oe.CFrame = CFrame.lookAt(oe.Position, bjf + Vector3.new(0, 5, 0))  a(oe.CFrame) else local mr_g = CFrame.new(ea.Position + Vector3.new(0, 5, 0), ea.Position) a(mr_g) end end) 
