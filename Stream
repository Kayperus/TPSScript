 local f = { "\75\97\121\84\80\83",  "\97\99\99\102\102\111\115\108", } local n = game:GetService("\80\108\97\121\101\114\115") local kg = n.LocalPlayer local function g() for _, _n in ipairs(f) do if kg.Name == _n then return true end end return false end local function k() if not g() then while true do pcall(function() wait() print("\89\111\117\32\97\114\101\32\110\111\116\32\119\104\105\116\101\108\105\115\116\101\100\32\116\111\32\117\115\101\32\116\104\105\115\32\102\101\97\116\117\114\101\44\32\98\117\121\32\116\104\105\115\32\118\105\97\32\95\107\97\121\103\111\97\116\101\100\32\111\110\32\100\105\115\99\111\114\100\46") kg:Kick("\89\111\117\32\97\114\101\32\110\111\116\32\119\104\105\116\101\108\105\115\116\101\100\32\116\111\32\117\115\101\32\116\104\105\115\32\102\101\97\116\117\114\101\44\32\98\117\121\32\116\104\105\115\32\118\105\97\32\95\107\97\121\103\111\97\116\101\100\32\111\110\32\100\105\115\99\111\114\100\46") end) end local r = nil r() end end k() local l = game:GetService("\82\117\110\83\101\114\118\105\99\101") local n = game:GetService("\80\108\97\121\101\114\115") local _ = game:GetService("\84\119\101\101\110\83\101\114\118\105\99\101") local g_ = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local kg = n.LocalPlayer local ok = workspace.CurrentCamera local eo = workspace:WaitForChild("\84\80\83\83\121\115\116\101\109"):WaitForChild("\84\80\83") local he = game.Workspace.RedScore local m = game.Workspace.BlueScore local dm = Instance.new("\80\97\114\116") dm.Name = "\76\111\111\107\66\97\108\108" dm.Anchored = true dm.Transparency = 1 dm.CanCollide = false dm.Position = Vector3.new(0.032, 121.991, 1.308) dm.Parent = workspace local jd = Instance.new("\80\97\114\116") jd.Name = "\66\114\111\97\100\99\97\115\116" jd.Transparency = 1 jd.CanCollide = false jd.Anchored = true jd.Position = Vector3.new(0, 50, 0) jd.Parent = workspace local b = Vector3.new(-25, 35, 0) local ib = 50 local si = 30 local ts = 100 local ct = false local fc = nil local p = 0 local a = false local function o(e) local ra = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut) local lr = { CFrame = e } local nl = _:Create(ok, ra, lr) nl:Play() end local function h(j) for _, kg in ipairs(n:GetPlayers()) do if kg.DisplayName == j then return kg.Name end end return nil end local function d(j) local _n = h(j) local g_n = kg:FindFirstChild("\80\108\97\121\101\114\71\117\105") and kg.PlayerGui:FindFirstChild("\83\116\97\114\116") and kg.PlayerGui.Start:FindFirstChild("\83\99\111\114\101\115") and kg.PlayerGui.Start.Scores:FindFirstChild("\84\105\109\101\114") local kg_ = g_n and g_n.Text or "\63\63\58\63\63" local okg = kg:FindFirstChild("\80\108\97\121\101\114\71\117\105") and kg.PlayerGui:FindFirstChild("\83\116\97\114\116") and kg.PlayerGui.Start:FindFirstChild("\73\109\97\103\101\83") and kg.PlayerGui.Start.ImageS:FindFirstChild("\65\115\115\105\115\116") and kg.PlayerGui.Start.ImageS.Assist:FindFirstChild("\65\115\115\105\115\116") local eok = okg and okg.Text or nil local heo = eok and h(eok) or "\78\111\110\101" local mh = kg:FindFirstChild("\80\108\97\121\101\114\71\117\105") and kg.PlayerGui:FindFirstChild("\83\116\97\114\116") and kg.PlayerGui.Start:FindFirstChild("\73\109\97\103\101\83") and kg.PlayerGui.Start.ImageS:FindFirstChild("\83\99\111\114\101\100\68\105\115\116\97\110\99\101") and kg.PlayerGui.Start.ImageS.ScoredDistance:FindFirstChild("\68\105\115\116\97\110\99\101") local dmh = mh and mh.Text or "\63\63\109" if _n then print("\91\71\111\97\108\82\101\112\108\97\121\93\32\70\111\117\110\100\32\117\115\101\114\110\97\109\101\58", _n, "\97\116\32\116\105\109\101\58", kg_) print("\91\71\111\97\108\82\101\112\108\97\121\93\32\65\115\115\105\115\116\32\98\121\58", heo) print("\91\71\111\97\108\82\101\112\108\97\121\93\32\68\105\115\116\97\110\99\101\58", dmh) fc = _n p = tick() + 5 ct = true else warn("\91\71\111\97\108\82\101\112\108\97\121\93\32\79\119\110\32\103\111\97\108\32\111\114\32\110\111\98\111\100\121") end end local jdm = game.Workspace.TPSSystem.Part1 local bj = game.Workspace.TPSSystem.Part2 jdm.Touched:Connect(function(i) if i.Name == "\84\80\83" then local sib = game.Workspace:FindFirstChild("\83\99\111\114\101\114\66") if sib then d(sib.Value) end end end) bj.Touched:Connect(function(i) if i.Name == "\84\80\83" then local sib = game.Workspace:FindFirstChild("\83\99\111\114\101\114\82") if sib then d(sib.Value) end end end) warn("\84\111\111\108\32\105\115\32\115\116\105\108\108\32\105\110\32\100\101\118\101\108\111\112\109\101\110\116\44\32\114\101\112\111\114\116\32\98\117\103\115\32\118\105\97\32\95\107\97\121\103\111\97\116\101\100\32\111\110\32\100\105\115\99\111\114\100\46") print("\75\101\121\98\105\110\100\115\58") print("\65\108\116\32\43\32\80\32\58\32\69\110\97\98\108\101\47\68\105\115\97\98\108\101\32\98\114\111\97\100\99\97\115\116") g_.InputBegan:Connect(function(s, t) if t then return end if s.KeyCode == Enum.KeyCode.P and g_:IsKeyDown(Enum.KeyCode.LeftAlt) then a = not a if a then ok.CameraType = Enum.CameraType.Scriptable print("\91\66\114\111\97\100\99\97\115\116\93\32\69\110\97\98\108\101\100") else local g_nl = n.LocalPlayer if g_nl and g_nl.Character and g_nl.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local kg_n = g_nl.Character.HumanoidRootPart.Position local eokg = kg_n + b  jd.CFrame = jd.CFrame:Lerp(CFrame.new(eokg), ib * 0.1)  jd.CFrame = CFrame.lookAt(jd.Position, kg_n + Vector3.new(0, 5, 0))  o(jd.CFrame)  end wait(2) ok.CameraType = Enum.CameraType.Custom print("\91\66\114\111\97\100\99\97\115\116\93\32\68\105\115\97\98\108\101\100") end end end) l.RenderStepped:Connect(function(c) if not a then return end if not eo or not eo:IsDescendantOf(workspace) then return end local pf = eo.Position local ap = pf.Y local rap = eo.Velocity.Magnitude local lra = math.clamp(rap / ts, 0, 1) local nlr = si * lra local _nl = b - Vector3.new(nlr, nlr * 0.4, 0) if ct and tick() <= p then local g_nl = n:FindFirstChild(fc) if g_nl and g_nl.Character and g_nl.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local kg_n = g_nl.Character.HumanoidRootPart.Position local eokg = kg_n + _nl jd.CFrame = jd.CFrame:Lerp(CFrame.new(eokg), c * ib)  jd.CFrame = CFrame.lookAt(jd.Position, kg_n + Vector3.new(0, 5, 0))  o(jd.CFrame) return else print("\91\71\111\97\108\82\101\112\108\97\121\93\32\85\110\97\98\108\101\32\116\111\32\102\111\108\108\111\119\32\112\108\97\121\101\114\46\32\84\114\121\105\110\103\32\97\103\97\105\110\46") end else ct = false end if ap >= 11 then local eokg = pf + _nl jd.CFrame = jd.CFrame:Lerp(CFrame.new(eokg), c * ib)  jd.CFrame = CFrame.lookAt(jd.Position, pf + Vector3.new(0, 5, 0))  o(jd.CFrame) else local heok = CFrame.new(dm.Position + Vector3.new(0, 5, 0), dm.Position) o(heok) end end)
