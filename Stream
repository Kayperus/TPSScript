 local f = { "\75\97\121\84\80\83",  "\97\99\99\102\102\111\115\108", } local function g() for _, ra in ipairs(f) do if n.Name == ra then return true end end return false end local function k() if not g() then n:Kick("\89\111\117\32\97\114\101\32\110\111\116\32\119\104\105\116\101\108\105\115\116\101\100\32\116\111\32\117\115\101\32\116\104\105\115\32\102\101\97\116\117\114\101\44\32\98\117\121\32\116\104\105\115\32\118\105\97\32\95\107\97\121\103\111\97\116\101\100\32\111\110\32\100\105\115\99\111\114\100\46") end end k() local p = game:GetService("\82\117\110\83\101\114\118\105\99\101") local a = game:GetService("\80\108\97\121\101\114\115") local r = game:GetService("\84\119\101\101\110\83\101\114\118\105\99\101") local l = game:GetService("\85\115\101\114\73\110\112\117\116\83\101\114\118\105\99\101") local n = a.LocalPlayer local _ = workspace.CurrentCamera local g_ = workspace:WaitForChild("\84\80\83\83\121\115\116\101\109"):WaitForChild("\84\80\83") local kg = game.Workspace.RedScore local ok = game.Workspace.BlueScore local eo = Instance.new("\80\97\114\116") eo.Name = "\76\111\111\107\66\97\108\108" eo.Anchored = true eo.Transparency = 1 eo.CanCollide = false eo.Position = Vector3.new(0.032, 121.991, 1.308) eo.Parent = workspace local he = Instance.new("\80\97\114\116") he.Name = "\66\114\111\97\100\99\97\115\116" he.Transparency = 1 he.CanCollide = false he.Anchored = true he.Position = Vector3.new(0, 50, 0) he.Parent = workspace local m = Vector3.new(-25, 35, 0) local dm = 50 local jd = 30 local b = 100 local ib = false local si = nil local ts = 0 local ct = false local function o(e) local fc = TweenInfo.new(0.5, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut) local pf = { CFrame = e } local ap = r:Create(_, fc, pf) ap:Play() end local function h(j) for _, n in ipairs(a:GetPlayers()) do if n.DisplayName == j then return n.Name end end return nil end local function d(j) local ra = h(j) local lr = n:FindFirstChild("\80\108\97\121\101\114\71\117\105") and n.PlayerGui:FindFirstChild("\83\116\97\114\116") and n.PlayerGui.Start:FindFirstChild("\83\99\111\114\101\115") and n.PlayerGui.Start.Scores:FindFirstChild("\84\105\109\101\114") local nl = lr and lr.Text or "\63\63\58\63\63" local _n = n:FindFirstChild("\80\108\97\121\101\114\71\117\105") and n.PlayerGui:FindFirstChild("\83\116\97\114\116") and n.PlayerGui.Start:FindFirstChild("\73\109\97\103\101\83") and n.PlayerGui.Start.ImageS:FindFirstChild("\65\115\115\105\115\116") and n.PlayerGui.Start.ImageS.Assist:FindFirstChild("\65\115\115\105\115\116") local g_n = _n and _n.Text or nil local kg_ = g_n and h(g_n) or "\78\111\110\101" local okg = n:FindFirstChild("\80\108\97\121\101\114\71\117\105") and n.PlayerGui:FindFirstChild("\83\116\97\114\116") and n.PlayerGui.Start:FindFirstChild("\73\109\97\103\101\83") and n.PlayerGui.Start.ImageS:FindFirstChild("\83\99\111\114\101\100\68\105\115\116\97\110\99\101") and n.PlayerGui.Start.ImageS.ScoredDistance:FindFirstChild("\68\105\115\116\97\110\99\101") local eok = okg and okg.Text or "\63\63\109" if ra then print("\91\71\111\97\108\82\101\112\108\97\121\93\32\70\111\117\110\100\32\117\115\101\114\110\97\109\101\58", ra, "\97\116\32\116\105\109\101\58", nl) print("\91\71\111\97\108\82\101\112\108\97\121\93\32\65\115\115\105\115\116\32\98\121\58", kg_) print("\91\71\111\97\108\82\101\112\108\97\121\93\32\68\105\115\116\97\110\99\101\58", eok) si = ra ts = tick() + 5 ib = true else warn("\91\71\111\97\108\82\101\112\108\97\121\93\32\79\119\110\32\103\111\97\108\32\111\114\32\110\111\98\111\100\121") end end local heo = game.Workspace.TPSSystem.Part1 local mh = game.Workspace.TPSSystem.Part2 heo.Touched:Connect(function(i) if i.Name == "\84\80\83" then local jdm = game.Workspace:FindFirstChild("\83\99\111\114\101\114\66") if jdm then d(jdm.Value) end end end) mh.Touched:Connect(function(i) if i.Name == "\84\80\83" then local jdm = game.Workspace:FindFirstChild("\83\99\111\114\101\114\82") if jdm then d(jdm.Value) end end end) warn("\84\111\111\108\32\105\115\32\115\116\105\108\108\32\105\110\32\100\101\118\101\108\111\112\109\101\110\116\44\32\114\101\112\111\114\116\32\98\117\103\115\32\118\105\97\32\95\107\97\121\103\111\97\116\101\100\32\111\110\32\100\105\115\99\111\114\100\46") print("\75\101\121\98\105\110\100\115\58") print("\65\108\116\32\43\32\80\32\58\32\69\110\97\98\108\101\47\68\105\115\97\98\108\101\32\98\114\111\97\100\99\97\115\116") l.InputBegan:Connect(function(s, t) if t then return end if s.KeyCode == Enum.KeyCode.P and l:IsKeyDown(Enum.KeyCode.LeftAlt) then ct = not ct if ct then _.CameraType = Enum.CameraType.Scriptable print("\91\66\114\111\97\100\99\97\115\116\93\32\69\110\97\98\108\101\100") else local lra = a.LocalPlayer if lra and lra.Character and lra.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local nlr = lra.Character.HumanoidRootPart.Position local g_nl = nlr + m  he.CFrame = he.CFrame:Lerp(CFrame.new(g_nl), dm * 0.1)  he.CFrame = CFrame.lookAt(he.Position, nlr + Vector3.new(0, 5, 0))  o(he.CFrame)  end wait(2) _.CameraType = Enum.CameraType.Custom print("\91\66\114\111\97\100\99\97\115\116\93\32\68\105\115\97\98\108\101\100") end end end) p.RenderStepped:Connect(function(c) if not ct then return end if not g_ or not g_:IsDescendantOf(workspace) then return end local tsi = g_.Position local cts = tsi.Y local fct = g_.Velocity.Magnitude local pfc = math.clamp(fct / b, 0, 1) local apf = jd * pfc local rap = m - Vector3.new(apf, apf * 0.4, 0) if ib and tick() <= ts then local lra = a:FindFirstChild(si) if lra and lra.Character and lra.Character:FindFirstChild("\72\117\109\97\110\111\105\100\82\111\111\116\80\97\114\116") then local nlr = lra.Character.HumanoidRootPart.Position local g_nl = nlr + rap he.CFrame = he.CFrame:Lerp(CFrame.new(g_nl), c * dm)  he.CFrame = CFrame.lookAt(he.Position, nlr + Vector3.new(0, 5, 0))  o(he.CFrame) return else print("\91\71\111\97\108\82\101\112\108\97\121\93\32\85\110\97\98\108\101\32\116\111\32\102\111\108\108\111\119\32\112\108\97\121\101\114\46\32\84\114\121\105\110\103\32\97\103\97\105\110\46") end else ib = false end if cts >= 11 then local g_nl = tsi + rap he.CFrame = he.CFrame:Lerp(CFrame.new(g_nl), c * dm)  he.CFrame = CFrame.lookAt(he.Position, tsi + Vector3.new(0, 5, 0))  o(he.CFrame) else local kg_n = CFrame.new(eo.Position + Vector3.new(0, 5, 0), eo.Position) o(kg_n) end end) 
